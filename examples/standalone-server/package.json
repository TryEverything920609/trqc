{
  "name": "@examples/standalone-server",
  "version": "1.3.0",
  "private": true,
  "scripts": {
    "dev:server": "nodemon -e ts -w . -x ts-node ./src/server.ts",
    "dev:client": "nodemon -e ts -w . -x 'wait-on tcp:2021 && ts-node ./src/client.ts'",
    "dev": "run-p dev:* --print-label"
  },
  "dependencies": {
    "@trpc/client": "^1.3.0",
    "@trpc/react": "^1.3.0",
    "@trpc/server": "^1.3.0",
    "@types/node-fetch": "^2.5.8",
    "abort-controller": "^3.0.0",
    "node-fetch": "^2.6.1",
    "typescript": "^4.1.3"
  },
  "alias": {
    "scheduler/tracing": "../../node_modules/scheduler/tracing-profiling"
  },
  "devDependencies": {
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "ts-node": "^9.1.1",
    "wait-on": "^5.2.1"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
