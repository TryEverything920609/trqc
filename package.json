{
  "name": "trpc",
  "version": "0.10.4",
  "description": "TypeScript-first toolkit for building RPC APIs",
  "main": "./lib/src/index.js",
  "types": "./lib/src/index.d.ts",
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vriad/trpc"
  },
  "author": "Colin McDonnell <colin@vriad.com>",
  "license": "MIT",
  "sideEffects": false,
  "bugs": {
    "url": "https://github.com/vriad/trpc/issues"
  },
  "homepage": "https://github.com/vriad/trpc",
  "dependencies": {
    "abort-controller": "^3.0.0",
    "axios": "^0.19.2",
    "react-query": "^3.5.16"
  },
  "tags": [],
  "keywords": [],
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules",
    "**/__tests__/*"
  ],
  "scripts": {
    "clean": "rm -rf lib/*",
    "build": "yarn run clean && tsc --p tsconfig.package.json",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --config jestconfig.json --coverage",
    "testone": "jest --config jestconfig.json",
    "dryrun": "nodemon -e ts -w . -x ts-node src/dryrun.ts",
    "serve": "nodemon -e ts -w . -x ts-node src/serve.ts",
    "prepublishOnly": "npm run build",
    "play": "nodemon -e ts -w . -x ts-node src/playground.ts",
    "playground:server": "nodemon -e ts -w . -x ts-node v2/playground/server.ts",
    "playground:client": "nodemon -e ts -w . -x 'wait-on tcp:2021 && ts-node v2/playground/client.ts'",
    "playground": "run-p playground:* --print-label"
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.7",
    "@types/jest": "^25.1.4",
    "@types/next": "^9.0.0",
    "@types/node-fetch": "^2.5.7",
    "@vercel/node": "^1.9.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jest": "^25.1.0",
    "node-fetch": "^2.6.1",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "ts-jest": "^25.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.0-beta",
    "wait-on": "^5.2.1",
    "zod": "^1.10.1"
  }
}
