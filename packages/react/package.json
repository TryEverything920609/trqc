{
  "name": "@trpc/react",
  "version": "10.0.0-proxy-beta.1",
  "description": "tRPC React lib",
  "author": "KATT",
  "license": "MIT",
  "homepage": "https://trpc.io",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trpc/trpc.git",
    "directory": "packages/react"
  },
  "scripts": {
    "ts-watch": "tsc --project tsconfig.watch.json"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./ssg": {
      "import": "./dist/ssg/index.mjs",
      "require": "./dist/ssg/index.js",
      "default": "./dist/ssg/index.js"
    },
    "./shared": {
      "import": "./dist/shared/index.mjs",
      "require": "./dist/shared/index.js",
      "default": "./dist/shared/index.js"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "ssg",
    "shared"
  ],
  "eslintConfig": {
    "rules": {
      "react-hooks/exhaustive-deps": "error",
      "no-restricted-imports": [
        "error",
        "@trpc/react"
      ]
    }
  },
  "peerDependencies": {
    "@tanstack/react-query": "^4.0.10",
    "@trpc/client": "^10.0.0-alpha.48",
    "@trpc/server": "^10.0.0-alpha.48",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@tanstack/react-query": "^4.0.10",
    "@trpc/client": "^10.0.0-proxy-beta.1",
    "@trpc/server": "^10.0.0-proxy-beta.1",
    "@types/express": "^4.17.12",
    "express": "^4.17.1",
    "next": "^12.3.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "zod": "^3.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
