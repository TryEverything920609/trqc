{
  "name": "@trpc/react",
  "version": "10.0.0-alpha.22",
  "description": "tRPC React lib",
  "author": "KATT",
  "license": "MIT",
  "homepage": "https://trpc.io",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trpc/trpc.git",
    "directory": "packages/react"
  },
  "scripts": {
    "build": "rollup -c"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./ssg": {
      "import": "./dist/ssg.mjs",
      "require": "./dist/ssg.js",
      "default": "./dist/ssg.js"
    }
  },
  "files": [
    "README.md",
    "dist",
    "ssg",
    "src"
  ],
  "eslintConfig": {
    "rules": {
      "react-hooks/exhaustive-deps": "error",
      "no-restricted-imports": [
        "error",
        "@trpc/react"
      ]
    }
  },
  "peerDependencies": {
    "@trpc/client": "^9.12.0",
    "@trpc/server": "^9.12.0",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "react-query": "^3.31.0 || ^4.0.0-alpha.4"
  },
  "dependencies": {
    "@babel/runtime": "^7.9.0"
  },
  "devDependencies": {
    "@trpc/client": "^10.0.0-alpha.22",
    "@trpc/server": "^10.0.0-alpha.22",
    "@types/express": "^4.17.12",
    "express": "^4.17.1",
    "next": "^12.1.6",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-query": "^3.27.0",
    "zod": "^3.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
