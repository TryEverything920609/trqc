{
  "name": "playground",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:server": "nodemon -e ts -w . -x ts-node ./server.ts",
    "dev:client": "nodemon -e ts -w . -x 'wait-on tcp:2021 && ts-node ./client.ts'",
    "dev": "run-p dev:* --print-label"
  },
  "dependencies": {
    "typescript": "^4.1.3"
  },
  "alias": {
    "trpc": "../../packages/trpc",
    "scheduler/tracing": "../node_modules/scheduler/tracing-profiling"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/react": "^17.0.0",
    "abort-controller": "^3.0.0",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "next": "^10.0.5",
    "node-fetch": "^2.6.1",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "ts-node": "^9.1.1",
    "wait-on": "^5.2.1"
  }
}
